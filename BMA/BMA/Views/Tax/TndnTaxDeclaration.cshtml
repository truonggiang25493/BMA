    <title>TndnTaxDeclaration</title>
    <!-- CSS -->
    <link href="~/Content/AdminLayout/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <style>
        .content {
            font-family: "Times New Roman", Times, serif;
