@using System.Collections.Specialized
@using BMA.Models
<!DOCTYPE html>
<html>
<head>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
    <title>@ViewBag.Title</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <link href="/Content/jquery-ui.css" rel="stylesheet" />
    <!-- Bootstrap 3.3.5 -->
    <link href="/Content/AdminLayout/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="/Content/font-awesome-4.3.0/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="/Content/AdminLayout/dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
    <!-- jvectormap -->
    <link href="/Content/AdminLayout/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    <!-- AdminLTE Skins -->
    <link href="/Content/AdminLayout/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
    <!-- Own css -->
    <link href="~/Content/style.css" rel="stylesheet" />
    <!-- Timesliders -->
    <link href="~/Content/timeslider.css" rel="stylesheet" />
    <!--DatetimePicker-->
    <link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <!--Bootstrap-chosen-->
    <link href="~/Content/bootstrap-chosen.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <a href="" class="logo">
                <span class="logo-mini">Quản lí</span>
                <span class="logo-lg">Quản lí tiệm bánh</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Notifications: style can be found in dropdown.less -->
                        <li class="dropdown notifications-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning">2</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header"></li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu">
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-file-o"></i> Có 5 đơn hàng mới
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-cube"></i> Có nguyên liệu trong kho sắp hết
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="/Content/AdminLayout/dist/img/Admin 128x128.png" class="user-image" alt="User Image">
                                <span class="hidden-xs">@(Session["User"] != null ? (Session["User"] as BMA.Models.User).Username : "")</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="/Content/AdminLayout/dist/img/Admin 128x128.png" class="img-circle" alt="User Image">
                                    <p>
                                        @(Session["User"] != null ? (Session["User"] as BMA.Models.User).Username : "") - @((Session["UserRole"]) == null ? "" : (((int)Session["UserRole"]) == 1 ? "Quản lý" : (((int)Session["UserRole"]) == 2 ? "Nhân viên" : "")))
                                        <small></small>
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="@Url.Action("StaffEditInfo", "Staff", new { id = (Session["User"] as BMA.Models.User).UserId })" class="btn btn-default btn-flat">Thông tin cá nhân</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="@Url.Action("Logout", "Account")" class="btn btn-default btn-flat">Đăng xuất</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="/Content/AdminLayout/dist/img/Admin 128x128.png" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>@(Session["User"] != null ? (Session["User"] as BMA.Models.User).Username : "")</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <div></div>
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="treeview">
                        <a href="">
                            <i class="fa fa-home"></i> <span>Quản lý cửa hàng</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "StoreInfor")"><i class="fa fa-home"></i>Thông tin cửa hàng</a></li>
                            <li>
                                <a href="@Url.Action("ConfigIndex", "StoreInfor")"><i class="fa fa-gears"></i>Cấu hình hệ thống</a>
                            </li>
                        </ul>
                    </li>
                    <li class="treeview" id="staff">
                        <a href="#">
                            <i class="fa fa-user"></i> <span>Nhân viên</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="listStaff"><a href="@Url.Action("StaffIndex", "Staff")"><i class="fa fa-users"></i>Danh sách nhân viên</a></li>
                            <li id="addStaff"><a href="@Url.Action("AddStaff", "Staff")"><i class="fa fa-user-plus"></i> Tạo tài khoản nhân viên</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="customer">
                        <a href="#">
                            <i class="fa fa-user"></i> <span>Khách hàng</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="listCustomer"><a href="@Url.Action("CustomerIndex","Customer")"><i class="fa fa-users"></i>Danh sách khách hàng</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-coffee"></i> <span>Sản phẩm</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "ManageProduct")"><i class="fa fa-coffee"></i>Danh sách sản phẩm</a></li>

                            <li><a href="@Url.Action("AddProduct", "ManageProduct")"><i class=" fa fa-plus"></i> Tạo mới sản phẩm</a></li>


                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-cube"></i> <span>Nguyên liệu</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","ManageMaterial")"><i class="fa fa-cubes"></i>Danh sách nguyên liệu</a></li>
                            <li><a href="@Url.Action("AddMaterial","ManageMaterial")"><i class=" fa fa-plus"></i>Tạo mới nguyên liệu</a></li>
                            <li><a href=""><i class="fa fa-home"></i>Quản lí kho</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="inputMaterial">
                        <a href="#">
                            <i class="fa fa-flask"></i> <span>Nguyên liệu đầu vào</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="listInputMaterial"><a href="@Url.Action("InputMaterialIndex","InputMaterial")"><i class="fa fa-cubes"></i>Danh sách nguyên liệu đầu vào</a></li>
                            <li id="addInputMaterial"><a href="@Url.Action("AddInputMaterial","InputMaterial")"><i class="fa fa-plus"></i>Thêm nguyên liệu đầu vào</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="supplier">
                        <a href="#">
                            <i class="fa fa-truck"></i> <span>Nhà cung cấp</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="listSupplier"><a href="@Url.Action("SupplierIndex","Supplier")"><i class="fa fa-list-ol"></i>Danh sách nhà cung cấp</a></li>
                            <li id="addSupplier"><a href="@Url.Action("AddSupplier","Supplier")"><i class="fa fa-plus"></i>Thêm nhà cung cấp mới</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="order">
                        <a href="#">
                            <i class="fa fa-file-o"></i> <span>Đơn hàng</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="orderList"><a href="@Url.Action("Index","Order")"><i class="fa fa-list-ol"></i>Danh sách đơn hàng</a></li>
                            <li id="addOrder"><a href="@Url.Action("Add", "Order" )"><i class="fa fa-plus"></i>Tạo mới đơn hàng</a></li>
                            <li><a href=""><i class="fa fa-thumbs-o-up"></i>Chấp nhận đơn hàng</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="inputBill">
                        <a href="#">
                            <i class="fa fa-file-text-o"></i> <span>Hóa đơn</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="listInputBill"><a href="@Url.Action("InputBillIndex", "InputBill")"><i class="fa fa-list-ol"></i>Danh sách hóa đơn</a></li>
                            <li id="addInputBill"><a href="@Url.Action("AddInputBill", "InputBill")"><i class="fa fa-plus"></i>Tạo mới hóa đơn</a></li>
                        </ul>
                    </li>
                    <li class="treeview" id="otherExpense">
                        <a href="@(Url.Action("Index", "OtherExpense"))">
                            <i class="fa fa-bar-chart"></i> <span>Chi phí khác</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                    </li>
                    <li class="treeview" id="report">
                        <a href="#">
                            <i class="fa fa-bar-chart"></i> <span>Thống kê</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="customerRevenue"><a href="@(Url.Action("ReviewRevenuePerCustomer", "Report"))"><i class="fa fa-usd"></i>Doanh thu theo khách hàng</a></li>
                            <li id="incomeProduct"><a href="@(Url.Action("ReviewRevenueByProduct", "Report"))"><i class="fa fa-usd"></i>Thu nhập theo sản phẩm</a></li>
                            <li id="incomeReport"><a href="@(Url.Action("ReviewIncomeByTime", "Report"))"><i class="fa fa-usd"></i>Thu nhập theo thời gian</a></li>
                            <li id="taxReport"><a href="@(Url.Action("Index","Tax"))"><i class="fa fa-file-text-o"></i>Thuế</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs"></ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane" id="control-sidebar-home-tab">
                </div><!-- /.tab-pane -->
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->
    <!--Small Modal -->
    <div class="modal fade bs-example-modal-sm" id="myModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm ">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="popup-title"></h4>
                </div>
                <div class="modal-body" id="popup-body"></div>
                <div class="modal-footer" id="popup-footer"></div>
            </div>
        </div>
    </div>
    <!--End of small modal-->
    <!--Standard Modal -->
    <div class="modal fade" id="myStandardModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="standard-popup-title"></h4>
                </div>
                <div class="modal-body" id="standard-popup-body"></div>
                <div class="modal-footer" id="standard-popup-footer"></div>
            </div>
        </div>
    </div>
    <!--End of standard modal-->
    <!--Large Modal -->
    <div class="modal fade bs-example-modal-lg" id="myLargeModal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="large-popup-title"></h4>
                </div>
                <div class="modal-body" id="large-popup-body"></div>
                <div class="modal-footer" id="large-popup-footer"></div>
            </div>
        </div>
    </div>
    <!--End of large modal-->
    <!-- jQuery 2.1.4 -->
    <script src="~/Content/AdminLayout/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.5 -->
    <script src="/Content/AdminLayout/bootstrap/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="/Content/AdminLayout/plugins/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Content/AdminLayout/dist/js/app.min.js"></script>
    <!-- Sparkline -->
    <script src="/Content/AdminLayout/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="/Content/AdminLayout/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="/Content/AdminLayout/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="/Content/AdminLayout/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="/Content/AdminLayout/plugins/chartjs/Chart.min.js"></script>
    <!-- DataTables -->
    <script src="/Content/AdminLayout/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="/Content/AdminLayout/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/Content/AdminLayout/dist/js/demo.js"></script>
    <!--Own js function-->
    <script src="~/Scripts/my-own-function.js"></script>
    <!--Noty js-->
    <script src="~/Scripts/jquery.noty.packaged.min.js"></script>
    <!--Timeslider-->
    <script src="~/Scripts/timeslider.js"></script>
    <!--DatetimePicker-->
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/moment-with-locales.min.js"></script>
    <script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>
    @RenderSection("InitScript", false)

    @RenderSection("AddInputMaterialScript", false)
    @RenderSection("OrderDetailScript", false)
    @RenderSection("CheckoutWithCustomerScript", false)
    @RenderSection("AddCustomerToOrderScript", false)
    @RenderSection("EditOrderScript", false)
    @RenderSection("AddOrderScript", false)
    @RenderSection("CustomerDetailScript", false)
    @RenderSection("InputBillDetailScript", false)
    @RenderSection("ReviewIncomeByTimeScript", false)
    @RenderSection("OtherExpenseIndexScript", false)
    @RenderSection("AddInputBillScript", false)
    @RenderSection("EditInputBillScript", false)
    @RenderSection("ReviewRevenuePerCustomerScript", false)
    @RenderSection("ReviewIncomeByProductScript", false)
    @RenderSection("TaxIndexScript", false)
    <script>
        $(function () {
            $('#example2').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": true,
                "ordering": true,
                "info": true,
                "autoWidth": false
            });
            $('#example1').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": true,
                "ordering": false,
                "info": true,
                "autoWidth": false
            });
            $('#datetimepicker').datetimepicker();
        });

        $(document).on("click", ".customer-table tbody tr", function () {
            $(this).addClass('selected').siblings().removeClass('selected');
        });





    </script>
    @if (ViewBag.TreeView != null)
    {
        <script type="text/javascript">
            $(document).find("#@(ViewBag.TreeView)").addClass('active');
        </script>
    }

    @if (ViewBag.TreeViewMenu != null)
    {
        <script type="text/javascript">
            $(document).find("#@(ViewBag.TreeViewMenu)").addClass('active');
        </script>
    }

</body>
</html>