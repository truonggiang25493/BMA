                                    <li> <a class="newProductMenuBlock" href="@Url.Action("Saltine", "Product")"> <img class="img-responsive" src="/Content/CustomerLayout/site/promo/BMTN.JPG" alt="product"> <span class="ProductMenuCaption"> <i class="fa fa-caret-right"> </i> Bánh mặn </span> </a> </li>
                                </ul>
                                <ul class="col-lg-3  col-sm-3 col-md-3 col-xs-4">
                                    <li> <a class="newProductMenuBlock" href="@Url.Action("Cookie","Product")"> <img class="img-responsive" src="/Content/CustomerLayout/site/promo/Pudding.jpg" alt="product"> <span class="ProductMenuCaption"> <i class="fa fa-caret-right"> </i> Bánh ngọt </span> </a> </li>
                                </ul>
                                <ul class="col-lg-3  col-sm-3 col-md-3 col-xs-4">
                                    <li> <a class="newProductMenuBlock" href="@Url.Action("Index","Product")"> <img class="img-responsive" src="/Content/CustomerLayout/site/promo/BBLN.jpg" alt="product"> <span class="ProductMenuCaption"> <i class="fa fa-caret-right"> </i> Đang khuyến mãi </span> </a> </li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li class="" id="procedure"> <a href="@Url.Action("Index", "Procedure")"> Quy trình đặt hàng </a> </li>
                </ul>

                <div class="nav navbar-nav navbar-right hidden-xs">
                    <div class="dropdown  cartMenu " style="">
                        <a class=" dropdown-toggle" data-toggle="dropdown" style="padding-right: 0px; padding-left: 5px;"> <i class="fa fa-shopping-cart"> </i> <span class="cartRespons">@Html.Action("CartPartial", "Cart") </span></a>
                    </div>
                </div>

            </div>

        </div>
    </div>

    @RenderBody()
