<body>
    <div class="modal signUpContent fade" id="ModalLogin" tabindex="-1" role="dialog">
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"> &times; </button>
                    <h3 class="modal-title-site text-center"> Đăng nhập </h3>
                </div>
                <form action='@Url.Action("Login", "Account", new { @strURL = Request.Url.ToString()})' id="loginForm" method="post">
                    <div class="modal-body">
                        <div class="form-group login-username">
                            <div>
                                <input name="txtAccount" id="login-user" class="form-control input" size="20" placeholder="Nhập tài khoản" type="text" required>
                            </div>
                        </div>
                        <div class="form-group login-password">
                            <div>
                                <input name="txtPassword" id="login-password" class="form-control input" size="20" placeholder="Nhập mật khẩu" type="password" required>
                            </div>
                        </div>
                        <div>
                            <div>
                                <input name="submit" class="btn  btn-block btn-lg btn-primary" value="ĐĂNG NHẬP" type="submit">
                            </div>
                        </div>

                    </div>
                </form>
                <div class="modal-footer">
                    <p class="text-center">
                        Chưa có tài khoản? <a data-toggle="modal" data-dismiss="modal" href="#ModalSignup"> Đăng ký. </a> <br>
                    </p>
                </div>
            </div>

        </div>

    </div>


    <div class="modal signUpContent fade" id="ModalSignup" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"> &times; </button>
                    <h3 class="modal-title-site text-center"> ĐĂNG KÝ </h3>
                </div>
                <div class="modal-body">
                    <div class="form-group reg-username">
                        <div>
                            <input name="txtUsername" class="form-control input" size="20" placeholder="Nhập tên tài khoản" type="text">
                        </div>
                    </div>
                    <div class="form-group reg-password">
                        <div>
                            <input name="txtPassword" class="form-control input" size="20" placeholder="Nhập mật khẩu" type="password">
                        </div>
                    </div>
                    <div class="form-group reg-password">
                        <div>
                            <input name="txtPasswordConfirm" class="form-control input" size="20" placeholder="Nhập lại mật khẩu" type="password">
                        </div>
                    </div>
                    <div class="form-group reg-email">
                        <div>
                            <input name="txtEmail" class="form-control input" size="20" placeholder="Nhập Email" type="text">
                        </div>
                    </div>
                    <div class="form-group reg-email">
                        <div>
                            <input name="txtPhonenumber" class="form-control input" size="20" placeholder="Nhập số điện thoại" type="text">
                        </div>
                    </div>
                    <div>
                        <div>
                            <input name="submit" class="btn  btn-block btn-lg btn-primary" value="ĐĂNG KÝ" type="submit">
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <p class="text-center"> Là thành viên? <a data-toggle="modal" data-dismiss="modal" href="#ModalLogin"> Đăng nhập </a> </p>
                </div>
            </div>

        </div>

    </div>


